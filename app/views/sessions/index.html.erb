<h1>Login</h1>
<% if @error_message %>
<h6> You entered an incorrect username or password. Please try again.</h6>
<% end %>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br/>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
